{
  "name": "acebase-client",
  "version": "1.17.1",
  "description": "Client to connect to an AceBase realtime database server",
  "main": "./src/index.js",
  "browser": {
    "socket.io-client": "socket.io-client/dist/socket.io.slim.js",
    "./src/request/index.js": "./src/request/browser.js",
    "./src/performance/index.js": "./src/performance/browser.js",
    "./src/base64/index.js": "./src/base64/browser.js"
  },
  "types": "./index.d.ts",
  "private": false,
  "repository": "github:appy-one/acebase-client",
  "scripts": {
    "build": "npm run browserify",
    "browserify": "browserify src/browser.js -o dist/browser.js --standalone acebaseclient --ignore rxjs && terser dist/browser.js -o dist/browser.min.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "npmfix": "npm i --package-lock-only"
  },
  "keywords": [
    "database",
    "db",
    "json",
    "binary",
    "object",
    "tree",
    "nosql",
    "embedded",
    "schemaless",
    "orm",
    "query",
    "index",
    "indexes",
    "fulltext",
    "geo",
    "transaction",
    "datastore",
    "fast",
    "low-memory",
    "realtime",
    "events",
    "users",
    "authentication",
    "rules",
    "authorization",
    "ssl",
    "https",
    "firebase alternative",
    "browser",
    "cache",
    "synchronization",
    "data proxy"
  ],
  "author": "Ewout Stortenbeker <me@appy.one> (http://appy.one)",
  "license": "MIT",
  "dependencies": {
    "acebase-core": "^1.22.4",
    "socket.io-client": "^2.4.0"
  },
  "devDependencies": {
    "browserify": "^17.0.0",
    "terser": "^5.15.0"
  }
}
